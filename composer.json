{
  "name": "endeken/ofx-php-parser",
  "description": "OFX PHP parser library",
  "type": "library",
  "license": "MIT",
  "keywords": ["ofx", "open financial exchange", "finance", "parser"],
  "authors": [
    {
      "name": "Wes Guirra",
      "email": "wes.guirra@endeken.com"
    }
  ],
  "require": {
    "ext-simplexml": "*",
    "ext-mbstring": "*",
    "ext-libxml": "*"
  },
  "autoload": {
    "psr-4": {
      "Endeken\\OFX\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "pestphp/pest": "^4.0",
    "phpstan/phpstan": "^2.0",
    "rector/rector": "^2.3"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "pest",
    "analyse": "phpstan analyse",
    "refactor": "rector process",
    "refactor-dry": "rector process --dry-run"
  }
}
